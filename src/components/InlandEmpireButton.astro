---
import Button from './Button.astro';

export interface Props {
	color?: 'primary' | 'secondary' | 'black' | 'white';
	variant?: 'fill' | 'outline';
	classes?: string;
}

const { color, variant, classes = '' } = Astro.props;
---

<Button
	href='https://www.eventbrite.com/e/grow-together-2024-conference-inland-empire-tickets-738046957317'
	isExternal={true}
	color={color}
	variant={variant}
	classes={classes}
	>Get Tickets</Button
>
