---
// Components
import Layout from '../layouts/Layout.astro';
import Hero from '../sections/Hero.astro';
import About from '../sections/About.astro';
import CTA from '../sections/CTA.astro';
import TeamMembers from '../sections/TeamMembers.astro';
import TrackInfo from '../sections/TrackInfo.astro';
import Testimonial from '../sections/Testimonial.astro';
import FinalCTA from '../sections/FinalCTA.astro';
import Sponsors from '../sections/Sponsors.astro';
import Footer from '../sections/Footer.astro';

// Styles
import 'aos/dist/aos.css';
---

<Layout title="Grow Together Conference 2024">
	<article>
		<Hero />
		<About />
		<CTA />
		<TeamMembers />
		<TrackInfo />
		<Testimonial />
		<FinalCTA />
		<Sponsors />
		<Footer />
	</article>
</Layout>

<script>
	import AOS from 'aos/dist/aos.js';
	AOS.init();
</script>
